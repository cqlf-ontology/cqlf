<?xml version="1.0"?>
<rdf:RDF xmlns="https://www.clarin.eu/standards/cqlf#"
     xml:base="https://www.clarin.eu/standards/cqlf"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
