        <rdfs:subClassOf rdf:resource="https://www.clarin.eu/standards/cqlf#Module"/>
    </owl:Class>
    


    <!-- https://www.clarin.eu/standards/cqlf#Paradigmatic -->

    <owl:Class rdf:about="https://www.clarin.eu/standards/cqlf#Paradigmatic">
